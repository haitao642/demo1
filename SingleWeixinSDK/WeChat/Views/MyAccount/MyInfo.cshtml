@{
    ViewBag.Title = "View";
}
@model Model.MyInfoM
<!doctype html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0;" name="viewport" />
    <title>@Model.Str_StoreName</title>

    <link rel="stylesheet" href="/Content/amazeui.min.css">
    <link rel="stylesheet" href="/Content/app.css">
    <link rel="stylesheet" href="~/Content/public.css">
    <link href="/Content/bootstrap.min.css" rel="stylesheet">
    <!--[if (gte IE 9)|!(IE)]><!-->
    <script src="/Scripts/jquery-1.10.2.min.js"></script>
    <!--<![endif]-->
    <!--[if lte IE 8 ]>
    <script src="http://libs.baidu.com/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://cdn.staticfile.org/modernizr/2.8.3/modernizr.js"></script>
    <script src="assets/js/amazeui.ie8polyfill.min.js"></script>
    <![endif]-->
    <script src="/Scripts/amazeui.min.js"></script>
    <script src="/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <div data-am-sticky>
        <div class="header">
            <div class="am-u-sm-2 am-vertical-align" style="height:100%;text-align:center;" onclick="back()">
                <img class="img_back am-vertical-align-middle" src="~/Images/back.png" />
                <text class="am-vertical-align-middle" style="font-size:16px;color:#ffffff;">返回</text>
            </div>
            <div class="am-u-sm-8 am-vertical-align" style="height:100%;text-align:center;">
                <text class="am-vertical-align-middle" style="font-size:16px;color:#ffffff;">我的资料</text>
            </div>
            <div class="am-u-sm-2" style="height:100%;">
            </div>
        </div>
    </div>
    <div class="am-g" style="padding:10px!important;font-weight:bold;">
        <div class="am-u-sm-12" style="height:60px;">
            <text class="" style="vertical-align:middle;line-height:60px;">头像</text>
            <img src="@Model.headerimg" style="height:40px;width:40px;float:right;vertical-align:middle;line-height:60px;" />
        </div>
        <div class="am-u-sm-12" style="height:1px;border-top:1px solid #d5d5d5;">
        </div>
        <div class="am-u-sm-12"  style="height:60px;">
            
            <text class="" style="vertical-align:middle;line-height:60px;">昵称</text>
            <text class="" style="color:#808080;float:right;vertical-align:middle;line-height:60px;">@Model.nickname</text>
        </div>
        <div class="am-u-sm-12 " style="height:1px;border-top:1px solid #d5d5d5;">
        </div>
        <div class="am-u-sm-12" style="height:60px;">
            <text class="" style="vertical-align:middle;line-height:60px;">姓名</text>
            <text class="" style="color:#808080;float:right;vertical-align:middle;line-height:60px;">@Model.cusname</text>
        </div>
        <div class="am-u-sm-12" style="height:1px;border-top:1px solid #d5d5d5;">
        </div>
        <div class="am-u-sm-12" style="height:60px;">
            <text class="" style="vertical-align:middle;line-height:60px;">身份证</text>
            <text class="" style="color:#808080;float:right;vertical-align:middle;line-height:60px;">@Model.strident</text>
        </div>
        <div class="am-u-sm-12" style="height:1px;border-top:1px solid #d5d5d5;">
        </div>
        <div class="am-u-sm-12" style="height:60px;">
            <text class="" style="vertical-align:middle;line-height:60px;">手机</text>
            <text class="" style="color:#808080;float:right;vertical-align:middle;line-height:60px;">@Model.strmobile</text>
        </div>
        <div class="am-u-sm-12" style="height:1px;border-top:1px solid #d5d5d5;">
        </div>
    </div>
    <script>
        function back() {
            var rawurl = '@Model.rawurl';
            location.href = rawurl;
        }
    </script>
</body>
</html>